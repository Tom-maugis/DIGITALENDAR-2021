{
  "parser": "babel-eslint",
  "extends": [
	"standard",
	"standard-react",
	"plugin:prettier/recommended",
	"prettier/react"
  ],
  "plugins": [
	"prettier",
	"react"
  ],
  "env": {
	"browser": true,
	"es6": true,
	"jest": true
  },
  "globals": {
	"__DEV__": false,
	"__TEST__": false,
	"__PROD__": false,
	"__COVERAGE__": false
  },
  "rules": {
	"valid-jsdoc": "warn",
	"react/prop-types": "warn",
	"linebreak-style": ["error", "unix"]
  },
  "settings": {
	"react": {
	  "pragma": "React",
	  "version": "16.8"
	}
  }
}
